    "version": "0.2.0",
    "configurations": [
        {
            "name": "panda (debug mode)",
            "request": "launch",
            "type": "dart",
            "args": [
                "--web-port=8080",
                "--dart-define-from-file=.env",
            ],
        },
        {
            "name": "panda (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": [
                "--web-port=8080",
                "--dart-define-from-file=.env",
            ],
        },
        {
            "name": "panda (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}